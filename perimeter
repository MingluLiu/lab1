package main
import ("fmt"; "math")
func distance(x1, y1, x2, y2 float64) float64 {
     a := x2 – x1
b := y2 – y1
     return math.Sqrt(a*a + b*b)
}
func rectanglePerimeter(x1, y1, x2, y2 float64)
float64 {
     l := distance(x1, y1, x1, y2)
     w := distance(x1, y1, x2, y1)
     return 2*l + 2*w
}
func circlePerimeter(x, y, r float64) float64 {
     return math.Pi * 2*r
}
